  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "App",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "port": 9229,
      "cwd": "${workspaceFolder}/src/app",
      "env": {
        "DISCOVER_URL": "http://localhost:5001"
      }
    },
    {
      "name": "Discover",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/discover/app.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/src/discover"
    }
  ]
}
